namespace op:
    actor StringConcatenation() ==>:
        String s;
        do
            s := s + "hello";
            s := "hello" + s;
            s := s + 1;
            s := 1 + s;
            s := s + 'a';
            s := 'a' + s;
            s := s + true;
            s := true + s;
            s := s + 3.14;
            s := 3.14 + s;
        end
    end
end
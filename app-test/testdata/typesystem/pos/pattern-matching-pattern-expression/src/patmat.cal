namespace patmat:
    function fun() --> int : 1 end
    type T : (int a, bool b) end
    T t;
    int n;
    actor PatternExpression() ==> int OUT:
        do
            OUT <--
                case t of
                   T(1 + n + fun(), not(n < 2)) : 1 end
                else
                    0
                end;
        end
    end
end